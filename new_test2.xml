<!-- this is a google linked CSE file -->
<?xml version="1.0" encoding="UTF-8" ?>
<GoogleCustomizations>
    <CustomSearchEngine>
        <Title>Wiki Plus One Tester</Title>
        <Description>for searching through websites</Description>
        <Context>
           <BackgroundLabels>
             <Label name="wiki_one" mode="FILTER" />
          </BackgroundLabels>
        </Context>
        <LookAndFeel nonprofit="true">
           <!-- In the spirit of solar, this search engine has a yellow background with orange titles -->
           <Colors url="#0B610B" background="#0040FF" border="#FF6600" title="#FF3300" text="#000000" visited="#663399" light="#FF0077"/>
        </LookAndFeel>
    </CustomSearchEngine>

   <!-- You need not include files to add annotations. Adding them inline as shown here will work as well -->
   <Annotations>
    <Annotation about="http://www.math.dartmouth.edu/*">
      <Label name="wiki_one"/>
    </Annotation>
   <Annotation about="http://www.cs.dartmouth.edu/*">
       <Label name="wiki_one"/>
   </Annotation>
  </Annotations>

</GoogleCustomizations>
